{
  "name": "colorix",
  "version": "1.2.0",
  "description": "Apply ANSI colors to strings!",
  "author": "cuppachino <cuppachino.dev@gmail.com>",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cuppachino/colorix.git"
  },
  "homepage": "https://github.com/Cuppachino/colorix#readme",
  "bugs": {
    "url": "https://github.com/Cuppachino/colorix/issues"
  },
  "keywords": [
    "ansi",
    "color",
    "colors",
    "console",
    "terminal",
    "pretty"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/modules/index.d.ts"
    },
    "./src/*": "./dist/*",
    "./src/modules/*": "./dist/modules/*",
    "./src/modules": {
      "import": "./dist/modules/index.mjs",
      "types": "./dist/modules/index.d.ts"
    },
    "./src/types/*": "./dist/types/*",
    "./src/types": {
      "import": "./dist/types/index.mjs",
      "types": "./dist/types/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "type": "module",
  "packageManager": "pnpm@7.25.0",
  "publishConfig": {},
  "scripts": {
    "dev": "nodemon --watch src --exec ts-node-esm ./src/index.mts",
    "build:swc": "ts-node ./src/index.mts",
    "build:tsc": "pnpm build:clean && tsc -p tsconfig.production.json",
    "build:clean": "tsc --build --clean",
    "start": "node ./dist/index.mjs"
  },
  "devDependencies": {
    "@swc/core": "^1.3.27",
    "@swc/helpers": "^0.4.14",
    "@types/node": "^18.11.18",
    "nodemon": "^2.0.20",
    "regenerator-runtime": "^0.13.11",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@cuppachino/type-space": "^1.10.1",
    "supports-color": "^9.3.1"
  }
}
